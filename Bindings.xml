<Bindings>
    <Binding
        name="VENDORALWAYSSELL"
        description="BINDING_DESC_VENDORALWAYSSELL" header="VENDORQUICKLIST" category="BINDING_CATEGORY_VENDOR">
        Vendor.AddTooltipItemToSellList()
    </Binding>
    <Binding
        name="VENDORNEVERSELL"
        description="BINDING_DESC_VENDORNEVERSELL" category="BINDING_CATEGORY_VENDOR">
        Vendor.AddTooltipItemToKeepList()
    </Binding>
    <Binding
        name="VENDORTOGGLEDESTROY"
        description="BINDING_DESC_VENDORTOGGLEDESTROY" category="BINDING_CATEGORY_VENDOR">
        Vendor.AddTooltipItemToDestroyList()
    </Binding>
    <Binding
        name="VENDORRUNDESTROY"
        description="BINDING_DESC_VENDORRUNDESTROY" category="BINDING_CATEGORY_VENDOR">
        Vendor.DestroyItems()
    </Binding>
    <Binding
        name="VENDORRUNAUTOSELL"
        description="BINDING_DESC_VENDORRUNAUTOSELL" category="BINDING_CATEGORY_VENDOR">
        Vendor.AutoSell()
    </Binding>
    <Binding
        name="VENDORRULES"
        description="BINDING_DESC_VENDORRULES" category="BINDING_CATEGORY_VENDOR">
        VendorRulesDialog:Toggle()
    </Binding>
    <!--@do-not-package@-->
    <Binding 
        name="Vendor Item Properties Debug Dump" 
        description="Dump item properties to console" category="BINDING_CATEGORY_VENDOR">
        Vendor:DumpItemPropertiesFromTooltip()
    </Binding>
    <!--@end-do-not-package@-->
</Bindings>
